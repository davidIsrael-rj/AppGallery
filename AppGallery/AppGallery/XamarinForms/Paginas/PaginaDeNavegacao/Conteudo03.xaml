<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Conteúdo 03"
             NavigationPage.HasBackButton="False"
             x:Class="AppGallery.XamarinForms.Paginas.PaginaDeNavegacao.Conteudo03">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Inserir Página na Pilha" Clicked="InserirPaginaPilha"/>
            <Button Text="Remover a Página da Pilha" Clicked="RemoverPaginaPilha"/>
            <Label Text="Página de Contúdo 03"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button Text="Voltar" Clicked="VoltarParaPaginaAnterior"/>
            <Button Text="Voltar para o ínicio" Clicked="VoltarParaOInicio"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>